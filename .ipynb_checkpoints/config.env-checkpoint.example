# repo-relative defaults; users can copy to config.env and tweak

# optional override: absolute path to raw audio (defaults to repo/data/audio/source)
# AUDIO_SOURCE_ROOT="/abs/path/to/source"

# optional override: absolute paths to csvs
# TP_CSV="/abs/path/train_tp.csv"
# FP_CSV="/abs/path/train_fp.csv"

# clip extraction
CLIP_SECONDS=3.0
CLIP_PAD="constant"

# models
MODELS=("birdnet" "audiomae" "biolingual")
RECURSIVE=true

# envs (examples; users set their own)
# CLIP_ENV="$HOME/venvs/clip-env"
# EMBED_ENV="$HOME/venvs/bacpipe-env"
# EVAL_ENV="$HOME/venvs/eval-env"
